<%- if controller_name != 'sessions' %>
  <%= link_to t(".log_in"), new_session_path(resource_name), class: "font-medium text-primary-600 hover:underline dark:text-primary-500" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to t(".sign_up"), new_registration_path(resource_name), class: "font-medium text-primary-600 hover:underline dark:text-primary-500" %><br />
<% end %>
